{
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "r.lsp.diagnostics": false,
    "workbench.startupEditor": "none",
    "r.session.emulateRStudioAPI": false,
    "r.workspaceViewer.showObjectSize": true,
    "terminal.integrated.tabs.enabled": false,
    "jupyter.askForKernelRestart": false,
    "[SAS]": {
        "files.encoding": "windows1252",
        "editor.tabSize": 4,
		"editor.insertSpaces": true,
		"editor.detectIndentation": false
    },
    "workbench.colorTheme": "Default High Contrast",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "python.linting.pylintCategorySeverity.refactor": "Error",
    "r.helpPanel.enableSyntaxHighlighting": false,
    "r.libPaths": [
        "C:/Program Files/R/R-4.1.0/library",
        "C:/Users/Conner.Smith/R/win-library/4.1"
    ],
    "workbench.localHistory.enabled": false,
    "terminal.integrated.shellIntegration.history": 0,
    "terminal.integrated.enablePersistentSessions": false,
